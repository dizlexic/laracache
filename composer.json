{
    "name": "jeandormehl/laracache",
    "license": "MIT",
    "description": "InterSystems Cach√© provider for Laravel (ODBC)",
    "keywords": [
        "laravel",
        "intersystems",
        "cache",
        "pdo",
        "odbc"
    ],
    "homepage": "https://github.com/jeandormehl/laracache",
    "authors": [
        {
            "name": "Jean Dormehl",
            "email": "jdormehl@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/database": "^10.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.16",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "7.4",
        "orchestra/testbench": "^8.3",
        "pestphp/pest": "^2.4",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Laracache\\": "src/"
        }
    },
    "scripts": {
        "test": "pest --compact",
        "test:coverage": "php -d pcov.enabled=1 ./vendor/bin/pest --coverage --compact",
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v2.0-dev"
        },       
        "laravel": {
            "providers": [
                "Laracache\\Cache\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
